<script>
    jQuery( document ).ready(function () {

        let this_version = jQuery('#type').data('version');
        let target = jQuery('#mirror').empty();
        jQuery('.download_section').each(function(){
            let optgroup = jQuery('<optgroup/>').attr('label', jQuery(this).find('h2').text());
            jQuery(this).find('a').each(function(){
                let href = jQuery(this).attr('href');
                if (href.indexOf('/mirror', 12) > 0) {
                    href = href.substring(0, href.indexOf('/mirror', 12));
                }
                if (href.indexOf('/releases', 12) > 0) {
                    href = href.substring(0, href.indexOf('/releases', 12));
                }
                let opt = jQuery('<option/>') ;
                opt.text(jQuery(this).text()).val(href + '/releases/' + this_version + '/');
                if (href.indexOf('mirror.ams1.nl.leaseweb.net') > 0) {
                    opt.attr('selected', 'selected');
                }
                optgroup.append(opt);
            });
            target.append(optgroup);
        });

        jQuery('.download-option').change(function(){
            let format = "img";
            if ( jQuery('#type').val() == "dvd" ) {
                format = "iso";
            }
            let filename = "OPNsense-"+jQuery('#type').data('version')+"-"+jQuery('#type').val()+"-"+jQuery('#architecture').val()+"."+format+".bz2";
            let sigtext = filename + " (SHA256) : " + jQuery('#type').find(':selected').data('hash');
            jQuery("#signaturetext").html(sigtext);
            jQuery("#download_opnsense").attr("href", jQuery('#mirror').val() + filename);
            jQuery("#download_opnsense").css('display', '');
            jQuery("#signature_block").css('display', '');
        }).change();
    });
</script>

